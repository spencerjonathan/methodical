<div>

	<form class="form-horizontal">
		<div class="form-group">
			<div class="col-md-10">
				<input id="method-name-input" class="typeahead form-control input-md"
					type="text" placeholder="Enter Method Name">
				</input>
			</div>
			<div class="col-md-2">
				<button id="add-method-name" type="button" class="btn btn-primary">Add</button>
			</div>
		</div>

		<div class="form-group form-vertical">
			<div class="col-md-10">
				<div class="panel panel-primary">
					<div class="panel-heading">
						<h3 class="panel-title">Selected Methods</h3>
					</div>
					<div class="panel-body" id="selected-methods-list">
						<select style="height:100px; width:100%;" class="no-scroll"
							multiple="true"></select>
					</div>
				</div>
			</div>
			<div class="col-md-2">
				<button id="remove-method-name" type="button" class="btn 
					btn-primary"
					onClick="removeMethodName()">Remove</button>
			</div>
		</div>

		<div class="form-group">
			<label class="col-md-4 control-label">Bob Place Notation</label>
			<div class="col-md-8">
				<input id="bobnotation" name="bobnotation" type="text"
					placeholder="14" class="form-control input-md" value="14"></input>

				<span class="help-block">Enter the bob\'s place notation here</span>
			</div>
		</div>

		<div class="form-group">
			<label class="col-md-4 control-label">Single Place Notation</label>
			<div class="col-md-8">
				<input id="singlenotation" name="singlenotation" type="text"
					placeholder="1234" class="form-control input-md" value="1234"></input>
				<span class="help-block">Enter the single\'s place notation here</span>
			</div>
		</div>
	</form>
</div>